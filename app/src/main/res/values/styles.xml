<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="APFActIdText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/text_size_no_f</item>
        <item name="android:textColor">@color/color_no_9_gray</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:maxLines">1</item>
        <item name="android:paddingTop">@dimen/padding_size_limitsmall</item>
    </style>

    <style name="BaseBtn">
        <item name="android:clickable">true</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="Btn" parent="BaseBtn">
        <item name="android:background">@drawable/selector_theme</item>
    </style>

    <style name="BtnRound" parent="BaseBtn">
        <item name="android:background">@drawable/selector_theme_small</item>
    </style>

</resources>
